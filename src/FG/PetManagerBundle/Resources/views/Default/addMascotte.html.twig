{% extends "FGPetManagerBundle:Default:index.html.twig" %}
{% block petmanager_title %}
		<h1>{{title}}</h1>
{% endblock %}
{% block petmanager_body %}
	{% for FlashMessage in app.session.flashbag.get('validation') %}
		<div class="well"><p>	{{FlashMessage}}</p></div>
	{% endfor %}

	{% if form is defined %} {{form(form)}}{% endif %}
{% endblock %}